<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MultiThreading.WebPage">
    
    <VerticalStackLayout x:Name="vsl" SizeChanged="vsl_SizeChanged">
        <WebView x:Name="MyWb">
            <WebView.Source>
                <HtmlWebViewSource x:Name="Myhtml"/>
            </WebView.Source>
        </WebView>
    </VerticalStackLayout>
    
</ContentPage>